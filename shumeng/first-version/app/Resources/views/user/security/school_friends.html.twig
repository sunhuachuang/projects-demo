{% extends 'user/base.html.twig' %}

{% block title %}edit{% endblock %}
{% block content_right %}
{% if is_granted("ROLE_USER") %}
<table class="table">
    <thead>
        <th>username</th>
        <th>position</th>
        <th>grade</th>
        <th>tel</th>
        <th>email</th>
    </thead>
    <tbody>
    {% for user in users %}
    <tr>
        <td>{{ user.username }}</td>
        <td>{{ user.position }}</td>
        <td>{{ user.grade }}</td>
        <td>{{ user.tel }}</td>
        <td>{{ user.email }}</td>
    </tr>
    {% endfor %}
    </tbody>
</table>
{% else %}
<p>guest cannot see</p>
{% endif %}
{% endblock %}